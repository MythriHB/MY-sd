include<stdio.h>
int main(){
    int arr[]={2,4,5,7,8};
    int n= sizeof(arr)/sizeof(arr[0]);
    int index = 13;
    int newvalue =6;
    if(index>=0 && index<n)
    {
        arr[index]=newvalue;
        for(int i=0;i<n;i++){ 
        printf("%d ",arr[i]);
        }
    }
        else
        {
            printf("kanasu l gowda\n");
        }
            return 0;
        }
